					<uri>http://edamontology.org/data_0945</uri>
					<term>Peptide identification</term>
				</data>
				<format>
					<uri>http://edamontology.org/format_3247</uri>
					<term>mzIdentML</term>
				</format>
			</input>
			<output>
				<data>
					<uri>http://edamontology.org/data_0945</uri>
					<term>Peptide identification</term>
				</data>
				<format>
					<uri>http://edamontology.org/format_3655</uri>
					<term>pepXML</term>
				</format>
				<format>
					<uri>http://edamontology.org/format_3247</uri>
